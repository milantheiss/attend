{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "https://localhost:8080",
			"webRoot": "${workspaceFolder}/client/src",
			"sourceMapPathOverrides": {
				"webpack:/*": "${webRoot}/*",
				"/./*": "${webRoot}/*",
				"/src/*": "${webRoot}/*",
				"/*": "*",
				"/./~/*": "${webRoot}/node_modules/*"
			},
			"runtimeArgs": ["--auto-ssl-client-auth"]
			// "preLaunchTask": "envoirement: start"
		}
	]
}
